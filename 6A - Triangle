#include<bits/stdc++.h>
using namespace std;
int main()
{
    int a,b,c,d;
    cin>>a>>b>>c>>d;
    if((a+b>c && b+c>a && a+c>b) || (a+b>d && b+d>a && a+d>b) ||(d+b>c && b+c>d && d+c>b) || (a+d>c && d+c>a && a+c>d))cout<<"TRIANGLE"<<endl;
    else if((a+b>=c && b+c>=a && a+c>=b) || (a+b>=d && b+d>=a && a+d>=b) ||(d+b>=c && b+c>=d && d+c>=b) || (a+d>=c && d+c>=a && a+c>=d))cout<<"SEGMENT"<<endl;
    else cout << "IMPOSSIBLE" << endl;
    return 0;
}
